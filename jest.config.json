{
    "preset": "jest-puppeteer",
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
        "json-summary",
        "text",
        "lcov"
    ],
    "collectCoverageFrom": [
        "**/*.{js}",
        "!**/node_modules/**",
        "!**/build/**",
        "!**/sample/**",
        "!**/jest-coverage-report/**",
        "!webpack.*.js"
    ]
}